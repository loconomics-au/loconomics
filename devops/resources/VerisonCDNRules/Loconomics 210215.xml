<policy>
    <rules>
        <rule>
            <description>HTTP to HTTPS redirect</description>
            <match.request.request-scheme.literal value="http">
                <feature.url.url-redirect source="/801089A6/cdne-loconomicsweb-001/(.*)" destination="https://%{host}/$1" code="301"/>
            </match.request.request-scheme.literal>
        </rule>
        <rule>
            <description>FILENAME-BASED CACHE BUSTING</description>
            <match.always>
                <feature.url.url-rewrite source="/801089A6/cdne-loconomicsweb-001/(.+)\.\d+(\.(js|css|png|jpg|gif|ico)$)" destination="/801089A6/cdne-loconomicsweb-001/$1$2"/>
            </match.always>
        </rule>
        <rule>
            <description>Redirect to Help</description>
            <match.always>
                <feature.url.url-redirect source="/801089A6/cdne-loconomicsweb-001/help(.*)" destination="https://%{host}/#!/help$1" code="302"/>
            </match.always>
        </rule>
        <rule>
            <description>Redirect to Terms</description>
            <match.always>
                <feature.url.url-redirect source="/801089A6/cdne-loconomicsweb-001/terms(.*)" destination="https://%{host}/#!/terms$1" code="302"/>
            </match.always>
        </rule>
        <rule>
            <description>Redirect to Privacy Settings</description>
