@{
    LcHelpers.TestArea();
    Layout = "_TestLayout.cshtml";
    
    var userID = WebSecurity.CurrentUserId;
    dynamic data = null;
    PageData["TestData"] = new Dictionary<string, dynamic>();
    
    switch (UrlData[0].ToLower()) {
        case "alerts":
            data = LcRest.Alert.GetActive(userID);
            break;
        case "positions":
            data = LcData.UserInfo.GetUserPositionsStatuses(userID);
            break;
        case "usercontact":
            data = LcData.UserInfo.GetUserRowWithContactData(userID);
            break;
    }
}
<div id="testindex">
    <ul>
        <li><a href="@Href("~/Tests/TestUserAlerts", "Alerts")">Alerts</a></li>
        <li><a href="@Href("~/Tests/TestUserAlerts", "Numbers")">Numbers</a></li>
        <li><a href="@Href("~/Tests/TestUserAlerts", "Positions")">GetUserPositionsStatuses</a></li>
        <li><a href="@Href("~/Tests/TestUserAlerts", "UserContact")">UserRowWithContactData</a></li>
    </ul>
</div>
<div id="testdata" style="margin-top: 40px; padding: 20px; border: 1px maroon solid">
    @ObjectInfo.Print(data)
</div>